{
  "name": "vsdist",
  "displayName": "VSDist",
  "description": "GetDist Tool for Visual Studio Code",
  "author": "Jiachen Bai",
  "publisher": "JiachenBai",
  "repository": "https://github.com/astrobai/vsdist",
  "icon": "images/icon.png",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.75.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "vsdist.start",
        "title": "VSDist: Analyse Chains"
      }
    ],
    "keybindings": [
      {
        "command": "vsdist.start",
        "when": "explorerResourceIsFolder"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "vsdist.start",
          "when": "explorerResourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "python -m pip install -r requirements.txt"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^14.0.0",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.0.0",
    "vscode": "^1.1.34"
  }
}
